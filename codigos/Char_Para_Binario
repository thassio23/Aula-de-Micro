#include<stdio.h>

void conta(char *a, int *vetor)
{
int arruma[8];
int binario[8];
int j = 7;
int i;
char b;
b = *a;

        for (i = 0; i <= 7 ;i++) {
            binario[i] = (b >> i) & 1;
            arruma[j] = binario[i];
            vetor[i] = arruma[i];
            j--;
        }
         for (i = 0; i <= 7 ;i++) {
         vetor[i] = arruma[i];
         }
    return;
}


main()

{
    char letra;
    int vetor[8];
    int i;

        for(;;){
        scanf("%c", &letra);
        conta(&letra, &vetor);
        for (i = 0; i <= 7 ;i++) {
        printf("%d", vetor[i]);
        }
        }

    return 0;
}
